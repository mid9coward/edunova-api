REFRESH_TOKEN_SECRET=
ACCESS_TOKEN_EXPIRES_IN=1d
REFRESH_TOKEN_EXPIRES_IN=7d
FRONTEND_URL=

MONGODB_URI=

GEMINI_API_KEY=

# SePay Configuration
