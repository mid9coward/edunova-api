# Coding Judge (Piston)
PISTON_API_URL=https://emkc.org/api/v2/piston
PISTON_TIMEOUT_MS=30000
PISTON_RETRY_ATTEMPTS=3
PISTON_SUBMIT_DELAY_MS=250
CODING_RESULT_MODE=leetcode

# Dual-limit profile:
# - learning: managed runtimes (java/.net/...) use relaxed engine memory for stability
# - judge: managed runtimes use bounded engine memory computed from problem limit
CODING_EXECUTION_PROFILE=learning
CODING_ENGINE_MANAGED_MEMORY_MULTIPLIER=2
CODING_ENGINE_MANAGED_MEMORY_MIN_MB=256
CODING_ENGINE_MANAGED_MEMORY_MAX_MB=1024
